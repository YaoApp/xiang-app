{
      "label": "数据看板",
      "version": "1.0.0",
      "description": "指标对比用于测试",
      "nodes": [
            {
                  "name": "云厂商市占率",
                  "engine": "xiang",
                  "query": {
                        "debug": true,
                        "select": [
                              "id",
                              "name",
                              "rate"
                        ],
                        "from": "$service",
                        "limit": 100
                  }
            },
            {
                  "name": "云厂商积分",
                  "engine": "xiang",
                  "query": {
                        "debug": true,
                        "select": [
                              "name",
                              "score"
                        ],
                        "from": "$service",
                        "limit": 100
                  }
            },
            {
                  "name": "云厂商使用率",
                  "engine": "xiang",
                  "query": {
                        "debug": true,
                        "select": [
                              "id",
                              "name",
                              "usage"
                        ],
                        "from": "$service",
                        "limit": 100
                  }
            },
            {
                  "name": "云厂商概览",
                  "engine": "xiang",
                  "query": {
                        "debug": true,
                        "select": [
                              "id",
                              "name",
                              "score",
                              "usage",
                              "rate"
                        ],
                        "from": "$service",
                        "limit": 100
                  }
            }
      ],
      "output": {
            "data": {
                  "云厂商市占率": "{{$res.云厂商市占率}}",
                  "云厂商积分": "{{$res.云厂商积分}}",
                  "云厂商使用率": "{{$res.云厂商使用率}}",
                  "云厂商概览": "{{$res.云厂商概览}}"
            },
            "query": "{{$in}}"
      },
      "apis": {
            "data": {
                  "disable": false,
                  "guard": "-",
                  "default": [
                        {
                              "from": "1980-01-02",
                              "to": "2050-12-31"
                        }
                  ]
            },
            "setting": {
                  "disable": false,
                  "guard": "-"
            }
      },
      "filters": {
            "时间区间": {
                  "label": "时间区间",
                  "bind": "range",
                  "input": {
                        "type": "rangePicker",
                        "props": {
                              "placeholder": [
                                    "开始时间",
                                    "结束时间"
                              ]
                        }
                  }
            }
      },
      "page": {
            "primary": "城市",
            "option": {
                  "screen": "/screen/screen.index"
            },
            "layout": {
                  "charts": [
                        {
                              "name": "云厂商概览",
                              "width": 24,
                              "type": "lineBar",
                              "props": {
                                    "height": 240,
                                    "x_key": "name",
                                    "axisLabel": {
                                          "interval": 0,
                                          "textStyle": {
                                                "fontSize": 12
                                          }
                                    },
                                    "series": [
                                          {
                                                "name": "score",
                                                "type": "bar",
                                                "itemStyle": {
                                                      "borderRadius": 6
                                                },
                                                "splitLine": {
                                                      "show": false
                                                },
                                                "axisLabel": {
                                                      "show": false
                                                },
                                                "label": {
                                                      "show": true,
                                                      "position": "top"
                                                }
                                          },
                                          {
                                                "name": "usage",
                                                "type": "line",
                                                "smooth": true,
                                                "symbolSize": 8,
                                                "itemStyle": {
                                                      "borderRadius": 6
                                                },
                                                "splitLine": {
                                                      "show": false
                                                },
                                                "axisLabel": {
                                                      "show": false
                                                }
                                          },
                                          {
                                                "name": "rate",
                                                "type": "line",
                                                "smooth": true,
                                                "symbolSize": 8,
                                                "itemStyle": {
                                                      "borderRadius": 6
                                                },
                                                "splitLine": {
                                                      "show": false
                                                },
                                                "axisLabel": {
                                                      "show": false
                                                }
                                          }
                                    ]
                              }
                        },
                        {
                              "name": "云厂商市占率",
                              "width": 12,
                              "type": "pie",
                              "swiper": true,
                              "speed": 3000,
                              "charts": [
                                    {
                                          "name": "玫瑰图",
                                          "type": "pie",
                                          "data_key": "云厂商市占率",
                                          "props": {
                                                "height": 240,
                                                "x_key": "name",
                                                "tooltip": {
                                                      "textStyle": {
                                                            "color": "#a2a5b9",
                                                            "fontSize": 12
                                                      },
                                                      "backgroundColor": "#232326",
                                                      "borderRadius": 6
                                                },
                                                "series": [
                                                      {
                                                            "name": "rate",
                                                            "type": "pie",
                                                            "roseType": "area",
                                                            "radius": [
                                                                  10,
                                                                  100
                                                            ],
                                                            "center": [
                                                                  "60%",
                                                                  "50%"
                                                            ],
                                                            "itemStyle": {
                                                                  "borderRadius": 3
                                                            }
                                                      }
                                                ]
                                          }
                                    },
                                    {
                                          "name": "柱状图",
                                          "type": "bar",
                                          "data_key": "云厂商使用率",
                                          "props": {
                                                "height": 240,
                                                "x_key": "name",
                                                "axisLabel": {
                                                      "interval": 0,
                                                      "textStyle": {
                                                            "fontSize": 10
                                                      }
                                                },
                                                "series": [
                                                      {
                                                            "name": "rate",
                                                            "type": "bar",
                                                            "colorBy": "data",
                                                            "itemStyle": {
                                                                  "borderRadius": 3
                                                            },
                                                            "splitLine": {
                                                                  "show": false
                                                            },
                                                            "axisLabel": {
                                                                  "show": false
                                                            }
                                                      }
                                                ]
                                          }
                                    }
                              ]
                        },
                        {
                              "name": "云厂商使用率",
                              "width": 12,
                              "type": "table",
                              "props": {
                                    "height": 240,
                                    "columns": [
                                          {
                                                "title": "ID",
                                                "dataIndex": "id"
                                          },
                                          {
                                                "title": "名称",
                                                "dataIndex": "name"
                                          },
                                          {
                                                "title": "使用率",
                                                "dataIndex": "usage"
                                          }
                                    ]
                              }
                        }
                  ]
            },
            "actions": {}
      }
}