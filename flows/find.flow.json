{
      "label": "查找合并",
      "version": "1.0.0",
      "description": "最新信息",
      "nodes": [
            {
                  "name": "服务",
                  "process": "models.service.Get",
                  "args": [
                        {
                              "withs": {
                                    "manu": {},
                                    "kind": {}
                              },
                              "wheres": [
                                    {
                                          "column": "id",
                                          "value": "{{$in.0}}"
                                    }
                              ]
                        }
                  ]
            },
            {
                  "name": "服务列表",
                  "process": "models.service.Get",
                  "args": [
                        {
                              "withs": {
                                    "manu": {},
                                    "kind": {}
                              },
                              "limit": 10
                        }
                  ],
                  "script": "data"
            },
            {
                  "name": "Test",
                  "process": "models.test.Get",
                  "script": "datatest",
                  "args": [
                        {}
                  ]
            },
            {
                  "name": "问卷",
                  "process": "xiang.helper.MapSet",
                  "args": [
                        "{{$res.Test}}",
                        "survey",
                        [
                              {
                                    "title": "您平时有饮酒的习惯吗？",
                                    "type": "单选",
                                    "children": {
                                          "columns": [
                                                {
                                                      "name": "选择项",
                                                      "width": 20,
                                                      "bind": "value",
                                                      "placeholder": "请输入饮酒习惯"
                                                },
                                                {
                                                      "name": "分数",
                                                      "width": 4,
                                                      "bind": "score",
                                                      "placeholder": "请输入分数"
                                                }
                                          ],
                                          "data": [
                                                {
                                                      "value": "有",
                                                      "score": 0
                                                },
                                                {
                                                      "value": "没有",
                                                      "score": 20
                                                }
                                          ]
                                    }
                              },
                              {
                                    "title": "您最后一次体检时间?",
                                    "type": "时间",
                                    "children": {
                                          "columns": [
                                                {
                                                      "name": "时间",
                                                      "width": 24,
                                                      "bind": "time",
                                                      "placeholder": "请选择体检时间"
                                                }
                                          ],
                                          "data": {}
                                    }
                              },
                              {
                                    "title": "您运动的频次?",
                                    "type": "数字",
                                    "children": {
                                          "columns": [
                                                {
                                                      "name": "频次",
                                                      "width": 12,
                                                      "bind": "min",
                                                      "placeholder": "最小值，不限定留空"
                                                },
                                                {
                                                      "name": "频次",
                                                      "width": 12,
                                                      "bind": "max",
                                                      "placeholder": "最大值，不限定留空"
                                                }
                                          ],
                                          "data": {
                                                "min": 0,
                                                "max": 10
                                          }
                                    }
                              },
                              {
                                    "title": "您的姓名?",
                                    "type": "文本",
                                    "children": {
                                          "columns": [
                                                {
                                                      "name": "姓名",
                                                      "width": 24,
                                                      "bind": "name",
                                                      "placeholder": "请输入姓名"
                                                }
                                          ],
                                          "data": {
                                                "name": "超级玛丽"
                                          }
                                    }
                              }
                        ]
                  ]
            }
      ],
      "output": "{{$res.问卷}}"
}